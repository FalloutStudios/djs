{
    "name": "djs",
    "private": true,
    "description": "A monorepo of discord.js utility packages",
    "scripts": {
        "clean": "npx rimraf ./packages/bin ./packages/dist",
        "build": "npm run clean && npx lerna run build && npx lerna bootstrap",
        "docs": "npx typedoc"
    },
    "devDependencies": {
        "@types/node": "^18.15.11",
        "discord.js": "^14.9.0",
        "lerna": "^6.6.1",
        "rimraf": "^4.4.1",
        "typedoc": "^0.23.28",
        "typescript": "^5.0.3"
    },
    "workspaces": [
        "packages/*"
    ],
    "dependencies": {
        "@discordjs/collection": "^1.5.0",
        "@falloutstudios/djs-scam-links": "^1.3.2",
        "fallout-utility": "^2.3.0"
    }
}
